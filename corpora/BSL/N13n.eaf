        <TIME_SLOT TIME_SLOT_ID="ts311" TIME_VALUE="763360"/>
        <TIME_SLOT TIME_SLOT_ID="ts312" TIME_VALUE="764690"/>
        <TIME_SLOT TIME_SLOT_ID="ts313" TIME_VALUE="873800"/>
        <TIME_SLOT TIME_SLOT_ID="ts314" TIME_VALUE="874900"/>
